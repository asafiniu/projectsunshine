.inline {
	display: inline
}

font.grey {
	color: #666666
}

table.header {
	background-color:#ffde00;
	width:760px
}

table.header tr.header_buttons td {
	background-color:#ffde00;
	width:760px
}

table.header tr.header_buttons td:not(.logo) {
	text-align: right;
	vertical-align: top
}

div#main, div#footer, div#header {
	width: 760px
}

table.menu {
	width:176px; 
	border:0;
	float:left
}

table.menu tr td.colored {
	background-color:#FFF8D4
}

table.home {
	width:584px; 
	border:0;
	float:right
}
table.home tr td.flash {
	width:290px; 
	height:230px; 
	background-color:#FFFFFF; 
	vertical-align:top
}

table.home tr td.intro {
	width:294px; 
	height:230px; 
	background-color:#fff8d4; 
	vertical-align:top
}

table.home tr td.ways_to_give {
	width:290px; height:130px;
	background-color:#fff3a6;
	vertical-align:top
}

table.home tr td.volunteer_intros {
	width:294px; height:130px;
	background-color:#ffee7f;
	vertical-align:top
}

table.home tr td.featured_header {
	width:584px; 
	height:40px
}

table.home tr td.featured_header font.featured_header_text {
	font-family:Verdana, Arial, Helvetica, sans-serif; 
	font-weight:bold; 
	color:#FF7E00; 
	margin-left:15px; 
	font-size:12px
}

table.home tr td.featured_header font.featured_header_text.lower {
	margin-top:15px
}

table.home tr td.news {
	width:584px; 
	vertical-align:top
}